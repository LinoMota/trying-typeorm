{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "outDir": "./build",
    "rootDir": "./src",
    "baseUrl": "./src",
    "strict": true,
    "rootDirs": ["src/entity"],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
  }
}